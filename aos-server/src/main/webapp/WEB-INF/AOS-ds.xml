<datasources xmlns="http://www.ironjacamar.org/doc/schema/datasources_1_1.xsd">
	<datasource jndi-name="java:jboss/datasources/AosDS"
		pool-name="PostgrePool" enabled="true">
		<connection-url>jdbc:postgresql://localhost:5432/aos</connection-url>
		<driver>postgres</driver>
		<security>
			<user-name>postgres</user-name>
			<password>postgres</password>
		</security>
		<pool>
			<max-pool-size>100</max-pool-size>
		</pool>
	</datasource>
</datasources>